<h1>Slots#index</h1>
<p>Find me in app/views/slots/index.html.erb</p>

<div class="container my-5">
  <%= simple_form_for(@slot) do |f| %>
    <%= f.input :start_time, label: "Please insert the date and start time" %>
    <%= f.input :duration, label: "How long will it take?" %>
    <%= f.submit %>
  <% end %>
</div>